<h1> HI IM A REAL PERSON NAMED <%= @user.name %></h1>
<h2> IM FROM <%= @user.location %></h2>
<br>
<h2>MY BEAUTIFUL DOGS</h2>
<% @user.dogs.each do |dog| %>

<div>
    <ul>
        <li>
            <%= link_to dog.name, dog_path(dog) %>
        </li>
    </ul>
<% end %>
</div>

<%= button_to 'BACK', users_path, method: :get %>

<h1>I LOVE TO GO TO THESE PARKS</h1>
<% @user.parks.each do |park|%>
    <div>
        <ul>
            <li>
                <%= link_to park.name, park_path(park) %>
            </li>
        </ul>
    </div>
<% end %>

<h1> I've HAD SOME GOOD DOG PARTIES TOO</h1>

<% @user.parties.each do |party| %>
    <div>
        <ul>
            <li>
                <%= link_to "Party with #{party.guest.name}", party_path(party) %>
            </li>
        </ul>
    </div>

<% end%>