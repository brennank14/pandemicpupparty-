<h1>PARKS</h1>
        <main class="cards grid-container">
            <% @parks.each do |park| %>
                <article class="card z-depth-3">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcQ4Oljj76S2b-MRGzHJVJiCa-rCgfjR9oQhog&usqp=CAU">
                    <div class="text">
                        <h3><%= park.name %> Park</h3>
                        <%= link_to park.name, park_path(park) %>
                    </div>
                </article>
            <% end %>
        </main>




